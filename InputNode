package annproj;

import java.util.ArrayList;

/**
 *
 * @author conlejd
 */
public class InputNode extends Node{
    private double inputVal;
    InputNode(){
        super();
    }
    @Override
    public void setInput(double i){
        inputVal = i;
    }
    @Override
    public double calculateOutput(){
        return inputVal;
    }
}
